{
  "name": "airconsole_game",
  "version": "0.0.1",
  "description": "Minigame-based party game for AirConsole.",
  "main": "src/js/screen.js",
  "scripts": {
    "build": "npm run buildScreen && npm run buildController",
    "buildScreen": "webpack --config config/screen.config.js && compass compile -c config/screen.config.rb",
    "buildController": "webpack --config config/controller.config.js && compass compile -c config/controller.config.rb",
    "clean": "rm ./build/ -rf",
    "cleanScreen": "rm ./build/screen/ -rf",
    "cleanController": "rm ./build/controller/ -rf"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrivateBrownBeard/Summer-2017-.git"
  },
  "keywords": [
    "airconsole",
    "minigame",
    "partygame"
  ],
  "author": "v o i d",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PrivateBrownBeard/Summer-2017-/issues"
  },
  "homepage": "https://github.com/PrivateBrownBeard/Summer-2017-#readme",
  "dependencies": {
    "phaser": "^3.0.0-alpha.2"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.1",
    "html-webpack-plugin": "^2.30.1",
    "script-loader": "^0.7.0",
    "webpack": "^3.4.1"
  }
}
